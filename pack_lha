.key nope
set version aga_64

set game us_champ_vball_${version}
set arc ${game}_HD.lha
if exists ${arc}
    echo deleting ${arc}...
    delete ${arc}
endif

; also archive with lha
lha a -r $arc ${game}_HD

set version aga_128

set game us_champ_vball_${version}
set arc ${game}_HD.lha
if exists ${arc}
    echo deleting ${arc}...
    delete ${arc}
endif

; also archive with lha
lha a -r $arc ${game}_HD

set version ecs

set game us_champ_vball_${version}
set arc ${game}_HD.lha
if exists ${arc}
    echo deleting ${arc}...
    delete ${arc}
endif

; also archive with lha
lha a -r $arc ${game}_HD

